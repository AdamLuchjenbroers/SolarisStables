<div xmlns:py="http://genshi.edgewall.org/" class="content">
  <h1>Technologies</h1>
  <py:for each="cat in techtree">
     <div class="techlist">
      <h2>${cat[0]}</h2>
      <py:for each="tier in cat[1]">
        <div class="tech_tier${tier[0]}">
          <py:for each="tech in tier[1]">
             <div class="tech">
               <span><a href="${baseURL}/${tech.urlname}">$tech.name</a></span>
               <span class="roll">${tech.base_difficulty}+</span>
             </div>
          </py:for>
        </div>
      </py:for>
    </div>
  </py:for>  
</div>